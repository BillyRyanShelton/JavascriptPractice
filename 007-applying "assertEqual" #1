function square(n) {
  return n * n;
}

function assertEqual(actual, expected, testName) {
  if(actual === expected) {
    console.log('passed')
  } else {
    console.log('FAILED [' + testName + '] Expected "' + expected + '", but got "' + actual + '"');
  }
}


assertEqual(square(5), 25, 'square for two positive integers');
assertEqual(square(0), 0, 'square for 0');
assertEqual(square(-5), 25, 'square for two negative integers');
